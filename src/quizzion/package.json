{
  "name": "quizzion",
  "version": "0.0.1",
  "description": "Quizzion 2.0 web application",
  "productName": "Quizzion 2.0",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Team a6-1",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./ ",
    "lint:fix": "eslint  --ext .js,.vue ./ --fix",
    "lint:report": "eslint --ext .js,.vue ./ -f node_modules/eslint-html-reporter/reporter.js -o report.html",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@vue/test-utils": "^1.0.3",
    "aos": "^2.3.4",
    "apexcharts": "^3.19.2",
    "axios": "^0.18.1",
    "eslint-html-reporter": "^0.7.4",
    "firebase": "^7.15.1",
    "md5": "^2.2.1",
    "qrcode.vue": "^1.7.0",
    "quasar": "^1.11.3",
    "socket.io-client": "^2.3.0",
    "vue-socket.io-extended": "^4.0.3",
    "uuid": "^8.1.0",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.5.3",
    "vue-carousel": "^0.18.0",
    "vue-router": "^3.3.2"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^1.0.0-beta.11",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "majestic": "^1.2.24",
    "eslint-plugin-cypress": "^2.10.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
}
